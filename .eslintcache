a the server to retrieve the information\r\n        axios.post('http://localhost:4000/api/myteam',newPlayer)\r\n        .then((res)=>{\r\n            console.log(res);\r\n        })\r\n        .catch((err)=>{\r\n            console.log(err);\r\n        });\r\n    }\r\n    render() {\r\n        return (\r\n            //forms that have been made with react features\r\n            //add players name, age, number and poster\r\n            <div className='App' style={{\r\n                display: \"flex\",\r\n                justifyContent: \"center\",\r\n                backgroundImage: \"url(\" +\r\n                \"https://wallpaperaccess.com/full/1127317.jpg\" + \")\",\r\n                width: '1903px',\r\n        height: '1000px',\r\n                alignItems: \"top\"}}>\r\n                <form onSubmit={this.onSubmit}>\r\n                    <div className='form-group'>\r\n                        <label>\r\n                            Player Name:\r\n                    </label>\r\n                        <input type='text'\r\n                            className='form-control form-control-lg'\r\n                            value={this.state.Name}\r\n                            onChange={this.onChangeName}>\r\n                        </input>\r\n                    </div>\r\n                    <div className='form-group'>\r\n                        <label> Player Age</label>\r\n                        <input type='text'\r\n                            className='form-control form-control-lg'\r\n                            value={this.state.Age}\r\n                            onChange={this.onChangeAge}>\r\n                        </input>\r\n                    </div>\r\n                    <div className='form-group'>\r\n                        <label>Squad Number</label>\r\n                        <textarea type='text'\r\n                            className='form-control form-control-lg'\r\n                            value={this.state.Number}\r\n        